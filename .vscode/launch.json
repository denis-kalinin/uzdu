{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Jest",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "runtimeArgs": [],
      "args": [
        "--runTestsByPath",
        "${relativeFileDirname}/${fileBasename}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
          "DEBUG": "true",
      }
    }
  ]
}